<div class="row">
  <div class="col-md-4">
    <h3>Import job orders from CSV</h3>
    <%= form_tag import_job_orders_path, multipart: true do %>
      <div class="form-group">
        <%= file_field_tag :job_orders_csv %>
      </div>
      <%= submit_tag "Import", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

